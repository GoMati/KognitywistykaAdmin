<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.admin.news.model.News" table="k_Aktualnosci">
		<id name="newsId" column="Id" type="long">
			<generator class="native"/>
		</id>
		<property name="title" type="java.lang.String">
			<column name="Tytul" not-null="true" length="80"/>
		</property>
		<property name="date" type="timestamp">
			<column name="CzasPublikacji" not-null="true" length="19"/>
		</property>
		<property name="content" type="java.lang.String">
			<column name="Tresc" not-null="true"/>
		</property>
		<many-to-one name="gallery" class="pl.edu.uj.kognitywistyka.admin.gallery.model.Gallery" fetch="select">
			<column name ="IdGalerii" not-null="false" />
		</many-to-one>
   </class>
</hibernate-mapping>