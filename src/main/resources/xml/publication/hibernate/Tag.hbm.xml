<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.admin.publication.model.Tag" table="k_tag">
		<id name="tagId" column="id" type="long">
			<generator class="native"/>
		</id>
		<property name="title" type="java.lang.String">
			<column name="title" not-null="true" length="80"/>
		</property>
		<bag name="publications" table="k_publication" fetch="select" inverse="true" order-by="tagId asc">
			<key>
				<column name="tagId" not-null="true"></column>
			</key>
			<many-to-many class="pl.edu.uj.kognitywistyka.admin.gallery.model.Publication"/>
		</bag>

   </class>
</hibernate-mapping>