<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.admin.aboutproject.model.Employee" table="k_employee">
		<id name="employeeId" column="Id" type="long">
			<generator class="native"/>
		</id>
		<property name="firstName" type="java.lang.String">
			<column name="firstName" not-null="true" length="30"/>
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="lastName" not-null="true" length="40"/>
		</property>
		<property name="title" type="java.lang.String">
			<column name="title" not-null="true" length="50"/>
		</property>
		<property name="description" type="java.lang.String">
			<column name="description" />
		</property>
		<property name="photo" type="java.lang.String">
			<column name="photo" />
		</property>
	
		<many-to-one name="position" class="pl.edu.uj.kognitywistyka.admin.aboutproject.model.Position" fetch="select">
			<column name="position" not-null="true" />
		</many-to-one>
   </class>
</hibernate-mapping>