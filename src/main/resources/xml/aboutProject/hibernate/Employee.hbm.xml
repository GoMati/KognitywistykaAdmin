<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.admin.aboutproject.model.Personnel" table="k_Kadra">
		<id name="employeeId" column="Id" type="long">
			<generator class="native"/>
		</id>
		<property name="firstName" type="java.lang.String">
			<column name="Imie" not-null="true" length="30"/>
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="Nazwisko" not-null="true" length="40"/>
		</property>
		<property name="title" type="java.lang.String">
			<column name="Tytul" not-null="true" length="50"/>
		</property>
		<property name="description" type="java.lang.String">
			<column name="Opis" />
		</property>
		<property name="photo" type="java.lang.String">
			<column name="Zdjecie" />
		</property>
	
		<many-to-one name="position" cascade="persist,merge,save-update" class="pl.edu.uj.kognitywistyka.admin.aboutproject.model.Position" column="Stanowisko"/>
   </class>
</hibernate-mapping>